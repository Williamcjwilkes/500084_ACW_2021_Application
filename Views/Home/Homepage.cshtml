@model _500084_ACW_2021_Web_Application.Models.User_Model

@{ ViewData["Title"] = "My Boards";

}
<div id='carouselIndicators' class='carousel slide' data-ride='carousel'>
    <ol class='carousel-indicators'>
        <li data-target='#carouselIndicators' data-slide-to='0' class='active'></li>
        <li data-target='#carouselIndicators' data-slide-to='1'></li>
        <li data-target='#carouselIndicators' data-slide-to='2'></li>
    </ol>
    <div class='carousel-inner'>
        <div class='carousel-item active'>
            <img class='card-img-top' src='https://www.lboro.ac.uk/media/wwwlboroacuk/external/content/services/studentservices/images/studservices-home-office-1400x400.jpg' alt='Placeholder img'>
            <div class='carousel-caption d-none d-md-block pb-5'>
                <h5>Welcome Back</h5>
                <p class='card-text'>Heres what you have missed</p>
            </div>
        </div>
        <div class='carousel-item '>
            <img class='card-img-top' src='https://www.mub.eps.manchester.ac.uk/uomchemistryblog/wp-content/uploads/sites/44/2020/07/PGTOfferHolderBlogs_Banner1400x400-1400x400.jpg' alt='Placeholder img'>
            <div class='carousel-caption d-none d-md-block pb-5'>
                <h5>Book and film society - Weekly Reader </h5>
                <p class='card-text'>Come down and share thoughts and opinion on this weeks book - The Hobbit</p>
            </div>
        </div>
        <div class='carousel-item '>
            <img class='card-img-top' src='https://www.mub.eps.manchester.ac.uk/maths-student-blogs/wp-content/uploads/sites/23/2018/07/DSG_9944-1-1400x400.jpg' alt='Placeholder img'>
            <div class='carousel-caption d-none d-md-block pb-5'>
                <h5>Mathmatic SOC - weekly equations night</h5>
                <p class='card-text'>This week we will be looking a Dijkstra's algorithm.'</p>
            </div>
        </div>
        <a class='carousel-control-prev' href='#carouselIndicators' role='button' data-slide='prev'>
            <span class='carousel-control-prev-icon' aria-hidden='true'></span>
            <span class='sr-only'>Previous</span>
        </a>
        <a class='carousel-control-next' href='#carouselIndicators' role='button' data-slide='next'>
            <span class='carousel-control-next-icon' aria-hidden='true'></span>
            <span class='sr-only'>Next</span>
        </a>
    </div>
</div>
<div class='container-fluid mt-4'>
    <div class='row row-cols-4'>
        <div class='col-lg-4 col-sm-6 mb-4 text-center'>
            <div class='card h-100 cardStyles'>
                <img class='card-img-top card-post-styles crd-rounded' src='https://d8it4huxumps7.cloudfront.net/bites/wp-content/banners/2021/8/611520e59e05b_coding.png?d=700x400' alt='Placeholder img'>
                <div class='card-body'>
                    <h4 class='card-title'>ComputerScienceSoc - Hackathon</h4>
                    <p class='card-text'>This week is the HAckathon, can you build a game over a week with a group of friends? </p>
                </div>
            </div>
        </div>
        <div class='col-lg-4 col-sm-6 mb-4 text-center'>
            <div class='card h-100'>
                <a href='#'><img class='card-img-top card-post-styles crd-rounded' src='https://forevergeek.com/wp-content/media/2018/10/Webp.net-resizeimage-1.jpg' alt='Placeholder img'></a>
                <div class='card-body'>
                    <h4 class='card-title'>Dr Who Soc - Social </h4>
                    <p class='card-text'>Come watch the Darleks invade earth in this weeks social !! - 12/12/21</p>
                </div>
            </div>
        </div>
        <div class='col-lg-4 col-sm-6 mb-4 text-center'>
            <div class='card h-100'>
                <a href='#'><img class='card-img-top card-post-styles crd-rounded' src='https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcScYsF0Y1hGFnfIYhquR3ETvwrTBpG2qMieQw&usqp=CAU' alt='Placeholder img'></a>
                <div class='card-body'>
                    <h4 class='card-title'>GardeningSOC - Social </h4>
                    <p class='card-text'>This weeks social is succulent based, come enjoy a pint and bring you plants!!</p>
                </div>
            </div>
        </div>
    </div>
</div>



